{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "nuvo-f",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "vercel-dev": "vercel dev --listen 3001",
    "build": "vite build",
    "vercel-build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start:api": "node server/index.js",
    "dev:api": "nodemon server/index.js",
    "dev:server": "nodemon server/index.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "distribute:tokens": "node --env-file=.env.distribution scripts/distributeTokens.js",
    "distribute:dry-run": "AIRDROP_AMOUNT_PER_USER=0 node --env-file=.env.distribution scripts/distributeTokens.js",
    "distribute:airdrop": "node --env-file=.env scripts/distributeAirdrop.js",
    "distribute:airdrop-dry": "node --env-file=.env scripts/distributeAirdrop.js --dry-run",
    "export:wallets": "node scripts/exportWallets.js"
  },
  "dependencies": {
    "@dodoex/widgets": "^3.1.0",
    "@google/genai": "^1.15.0",
    "@mozilla/readability": "^0.6.0",
    "@types/lodash": "^4.17.16",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ethers": "^6.13.5",
    "express": "^5.1.0",
    "firebase": "^11.3.0",
    "framer-motion": "^12.6.3",
    "github-markdown-css": "^5.8.1",
    "jsdom": "^26.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.540.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-responsive": "^10.0.1",
    "react-router-dom": "^6.30.1",
    "react-slick": "^0.30.3",
    "react-syntax-highlighter": "^15.6.1",
    "react-tsparticles": "^2.12.2",
    "recharts": "^2.15.3",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tsparticles": "^3.8.1",
    "ws": "^8.18.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "buffer": "^6.0.3",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "util": "^0.12.5",
    "vite": "^6.2.6"
  }
}
