# Distribution Configuration Template
# Copy this file to .env.distribution and replace with your actual values

# Distribution Configuration
DISTRIBUTOR_PRIVATE_KEY=your_private_key_here
TOKEN_CONTRACT_ADDRESS=your_token_contract_address_here
AIRDROP_AMOUNT_PER_USER=10
NETWORK=polygon
AIRDROP_TYPE=tokens

# Contract Addresses - Replace with your deployed contract addresses
VITE_STAKING_ADDRESS=0x0000000000000000000000000000000000000000
VITE_STAKING_ADDRESS_V2=0x0000000000000000000000000000000000000000
VITE_AIRDROP_ADDRESS=0x0000000000000000000000000000000000000000
VITE_TREASURY_ADDRESS=0x0000000000000000000000000000000000000000
VITE_TOKENIZATION_ADDRESS=0x0000000000000000000000000000000000000000
VITE_TOKENIZATION_ADDRESS_V2=0x0000000000000000000000000000000000000000
VITE_NUVO_TOKEN=0x0000000000000000000000000000000000000000

# Blockchain Configuration - Get from Alchemy Dashboard
VITE_ALCHEMY=your_alchemy_api_key_here
VITE_CHAIN_ID=137

# Firebase Configuration - Get from Firebase Console
VITE_FIREBASE=your_firebase_api_key_here
VITE_FIREBASE_AD=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=1:your_sender_id:web:your_app_id
VITE_FIREBASE_MEASUREMENT_ID=G-YOUR_MEASUREMENT_ID

# IPFS Configuration (Pinata) - Get from Pinata Dashboard
VITE_PINATA=your_pinata_api_key_here
VITE_PINATA_SK=your_pinata_secret_key_here

# Additional APIs
VITE_SWAP_DODOEX=your_dodoex_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Environment
VITE_NODE_ENV=production
NODE_ENV=production
PORT=3001
